{% set strategy = 'html' %}
<!doctype html>
<html lang="en">
<head>
  {% block head %}
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="Multi-EAR">

  <meta name="mobile-web-app-capable" content="yes">

  <title>{% block title %}{% endblock %} {% if hostname %}{{hostname}}{% endif %}</title>

  <!-- Favicon -->
  <link rel="apple-touch-icon" sizes="180x180" href="{{ url_for('static', filename='icon/apple-touch-icon.png') }}">
  <link rel="icon" type="image/png" sizes="32x32" href="{{ url_for('static', filename='icon/favicon-32x32.png') }}">
  <link rel="icon" type="image/png" sizes="16x16" href="{{ url_for('static', filename='icon/favicon-16x16.png') }}">
  <link rel="manifest" href="site.webmanifest">
  <link rel="mask-icon" href="{{ url_for('static', filename='icon/safari-pinned-tab.svg') }}" color="#00a6d6">
  <meta name="msapplication-TileColor" content="#da532c">
  <meta name="theme-color" content="#ffffff">

  <!-- Bootstrap core CSS -->
  <link rel="stylesheet" type="text/css" href="{{ url_for('static', filename='3rd/bootstrap-icons-v1.7.0/bootstrap-icons.css') }}">
  <link rel="stylesheet" type="text/css" href="{{ url_for('static', filename='css/bootstrap.min.css') }}">
  <link rel="stylesheet" type="text/css" href="{{ url_for('static', filename='css/multi-ear.css') }}">

  <!-- Custom styles for this template -->
  {% block styles %}{% endblock %}

  {% endblock %}
</head>
<body class="bg-light">
  <nav class="navbar navbar-expand-md flex-column navbar-dark bg-dark p-md-0">
    <div class="container-fluid">
      <a class="navbar-brand d-md-none" href="#" onclick="return false;">{{hostname}}</a>
      <a class="navbar-brand d-none d-md-block" href="#" onclick="return false;">{{hostname|replace('-','<br>')}}</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbar"
       aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse flex-column text-center mt-2 mt-md-0" id="navbar">
        <ul class="nav nav-pills nav-flush flex-column border-top mb-auto">
          <li class="nav-item border-bottom">
            <a class="active nav-link" aria-current="page" href="#" data-bs-toggle="tab" data-bs-target="home" title="Home">
              <i class="bi bi-house-door"></i><span class="d-md-none"> Home</span>
            </a>
          </li>
          <li class="nav-item border-bottom">
            <a class="nav-link" href="#pcb" data-bs-toggle="tab" data-bs-target="pcb" title="MEMS on a PCB">
              <i class="bi bi-cpu"></i><span class="d-md-none"> MEMS on a PCB</span>
            </a>
          </li>
          <li class="nav-item border-bottom">
            <a class="nav-link" href="#dashboard" data-bs-toggle="tab" data-bs-target="dashboard" title="Data">
              <i class="bi bi-graph-up"></i><span class="d-md-none"> Data</span>
            </a>
          </li>
          <li class="nav-item border-bottom">
            <a class="nav-link" href="#wifi" data-bs-toggle="tab" data-bs-target="wifi" title="Wi-Fi control">
              <i class="bi bi-wifi"></i><span class="d-md-none"> Wi-Fi control</span>
            </a>
          </li>
          <li class="nav-item border-bottom">
            <a class="nav-link" href="#status" data-bs-toggle="tab" data-bs-target="status" title="Status monitoring">
              <i class="bi bi-journal-code"></i><span class="d-md-none"> Status monitoring</span>
            </a>
          </li>
        </ul>
        <ul class="nav nav-pills flex-column border-top">
          <li class="nav-item border-bottom">
            <a class="nav-link" href="http://{{hostname}}.local:3000"
             title="Grafana" target="_blank" rel="noopener noreferrer">
              <i class="bi bi-server" alt="Grafana"></i><span class="d-md-none"> Grafana server</span>
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="https://github.com/Multi-EAR"
             title="Github" target="_blank" rel="noopener noreferrer">
              <i class="bi bi-github" alt="Github"></i><span class="d-md-none"> Github project</span>
            </a>
          </li>
        </ul>
      </div>
    </div>
  </nav>
  <main class="container-fluid ps-md-4" id="content">
    {% block main %}..{% endblock %}
  </main>

  <!-- JS -->
  <script src="{{ url_for('static', filename='3rd/bootstrap-v5.1.3/bootstrap.bundle.min.js') }}"></script>
  <script src="{{ url_for('static', filename='3rd/highcharts-v9.3.1/highcharts.js') }}"></script>
  <script src="{{ url_for('static', filename='3rd/highcharts-v9.3.1/modules/boost.js') }}"></script>
  <script src="{{ url_for('static', filename='3rd/highcharts-v9.3.1/modules/data.js') }}"></script>
  <script src="{{ url_for('static', filename='3rd/highcharts-v9.3.1/modules/exporting.js') }}"></script>
  <script src="{{ url_for('static', filename='3rd/highcharts-v9.3.1/modules/export-data.js') }}"></script>
  <script src="{{ url_for('static', filename='3rd/crypto-js-v4.1.1/core.js') }}"></script>
  <script src="{{ url_for('static', filename='3rd/crypto-js-v4.1.1/sha256.js') }}"></script>
  <script src="{{ url_for('static', filename='js/get-response.js') }}"></script>
  <script src="{{ url_for('static', filename='js/show-password-toggle.min.js') }}"></script>
  <script src="{{ url_for('static', filename='js/multi-ear.js') }}"></script>

  <!-- Custom scripts for this template -->
  {% block script %}{% endblock %}
</body>
</html>
