[Unit]
Description=Automatically generates a wlan hotspot when no a valid ssid is in range
After=multi-user.target

[Service]
Type=oneshot
RemainAfterExit=yes
User=tud
Group=tud
ExecStart=/home/tud/.py37/bin/autohotspot
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=multi-ear-wifi

[Install]
WantedBy=multi-user.target
